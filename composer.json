{
    "name": "frozzare/redirects",
    "type": "utility",
    "license": "MIT",
    "description": "Netlify's _redirects file format parser",
    "keywords": [
        "feature",
        "features"
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Frozzare\\Redirects\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "lint": "vendor/bin/phpcs --standard=PSR2 src",
        "format": "vendor/bin/phpcbf --standard=PSR2 src tests",
        "test": "vendor/bin/phpunit"
    }
}
